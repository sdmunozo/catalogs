{
  "builds": [
    {
      "src": "build/web/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/build/web/index.html",
      "status": 200
    }
  ],
  
   "rewrites": [
     {
       "source": "/(.*)",
       "destination": "/index.html"
     }
  ]
}
  
